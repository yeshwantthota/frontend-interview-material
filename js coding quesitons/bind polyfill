function x (...args){
    console.log(this.a)
    console.log(args)
}


const obj = {
    a: 10
}

// x.bind(obj)()


Function.prototype.myBind = function (context, ...args){
    console.log("context", context)
    console.log(this)
    const fn = this;
    
    return function (){
        fn.apply(context, [...args])
    }
}

x.myBind(obj, 20)()
